@using OSBLE.Resources
@using OSBLE.Models.Assignments
@model DynamicDictionary
@{
    dynamic dataModel = Model;
    int assignmentID = dataModel.PreviousAssignment.ID;
    string assignmentName = dataModel.PreviousAssignment.name;
}

<tr>
<th>Preceding Assignment</th>
<td>@Html.ActionLink("Go to preceding assignment - \"" + assignmentName + "\"", "Index", "Home", new { area = "AssignmentDetails", assignmentId = assignmentID }, null)</td>
</tr>