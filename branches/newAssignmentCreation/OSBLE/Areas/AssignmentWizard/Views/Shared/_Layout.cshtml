@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Header
{
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/json2.min.js")" type="text/javascript"></script>
    @RenderSection("Header", false)
}
<div id="WizardContent">
@using (Html.BeginForm())
{
@RenderBody()
<div id="WizardBottomNav" style="clear:both;">
        <input type="submit" value="Previous" name="@ViewBag.PreviousWizardButton" />
        <input type="submit" value="Next" name="@ViewBag.NextWizardButton" />
</div>
}

<!--end of "WizardContent"-->
</div>