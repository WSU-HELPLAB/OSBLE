@{
    ViewBag.Title = "Critical Review";
    string annotateUrl = ViewBag.AnnotateUrl;
}
<iframe id="AnnotateFrame" src="@(annotateUrl)" style="width:99%;"></iframe>
<script type="text/javascript">
    $(document).ready(scaleAnnotateFrame);
    $(window).resize(scaleAnnotateFrame);

    function scaleAnnotateFrame() {
        $('#AnnotateFrame').width($(window).width() - 20);
        $('#AnnotateFrame').height($(window).height() - 110);
    }
</script>