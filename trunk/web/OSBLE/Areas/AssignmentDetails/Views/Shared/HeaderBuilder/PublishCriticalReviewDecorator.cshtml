@using OSBLE.Resources
@using OSBLE.Models.Assignments
@model DynamicDictionary
@{
    dynamic dataModel = Model;
    int assignmentId = dataModel.PublishCR.assignmentID;
    string linkDisplayValue = dataModel.PublishCR.PublishButtonDisplayValue;
    
}

<tr>
    <th>Critical Review Publish</th>
    <td>
        <text>dataModel.PublishCR.PublishCR.PublishStatus</text>
        @Html.ActionLink(linkDisplayValue,
                    "PublishAllCriticalReviews",
                    "Assignment",
                    new { area = "", assignmentID = assignmentId },
                    new { })
    </td>
</tr>

