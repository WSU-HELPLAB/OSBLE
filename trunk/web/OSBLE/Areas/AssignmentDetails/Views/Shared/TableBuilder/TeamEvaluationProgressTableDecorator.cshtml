@using OSBLE.Resources
@using OSBLE.Models.Assignments
@model DynamicDictionary
@{
    dynamic dataModel = Model;
    IAssignmentTeam currentTeam = dataModel.AssignmentTeam as IAssignmentTeam;
    string linkText = string.Format("{0} of {1}", dataModel.TeamEvaluationProgress.CompletedEvaluations, dataModel.TeamEvaluationProgress.TotalEvaluations);
}
<td>
@Html.ActionLink(linkText, "TeacherTeamEvaluation", "Assignment", new { teamId = currentTeam.Team.ID, assignmentId = currentTeam.Assignment.ID }, new { })
</td>