@using OSBLE.Resources
@using OSBLE.Models.Assignments
@model DynamicDictionary
@{
    dynamic dataModel = Model;
    IAssignmentTeam currentTeam = dataModel.TeamEvaluationProgress.AssignmentTeam as IAssignmentTeam;
    string linkText = string.Format("{0} of {1}", dataModel.TeamEvaluationProgress.CompletedEvaluations, dataModel.TeamEvaluationProgress.TotalEvaluations);
}
<td>
@Html.ActionLink(linkText, "TeacherTeamEvaluation", "Assignment", new { precedingTeamId = currentTeam.Team.ID, TeamEvaluationAssignmentId = dataModel.TeamEvaluationProgress.AssignmentId, area = "" }, new { })
</td>