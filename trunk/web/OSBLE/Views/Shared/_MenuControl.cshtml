@using OSBLE.Models
<div id="tabs">
    @using OSBLE.Controllers
    @foreach (OSBLEController.MenuItem m in ViewBag.Menu)
    {
        if ( ((ViewBag.CurrentUser != null) && (!m.AdminOnly || ViewBag.CurrentUser.IsAdmin))
           &&
           (!m.ModifierOnly || ((ViewBag.ActiveCourse != null) && ViewBag.ActiveCourse.CourseRole.CanModify))
           &&
		   (!m.GraderOnly || ((ViewBag.ActiveCourse != null) && ViewBag.ActiveCourse.CourseRole.CanGrade))
           &&
           (!m.ViewerOnly || ((ViewBag.ActiveCourse != null) && ViewBag.ActiveCourse.CourseRole.CanSeeAll))
           &&
           ((!m.CommunityOnlyPage && !m.NotInCommunityPage) || (m.NotInCommunityPage && !(ViewBag.ActiveCourse.Course is Community) || (m.CommunityOnlyPage && ViewBag.ActiveCourse.Course is Community)))
          ) 
        {
        @Html.ActionLink(@m.Name, @m.Action, @m.Controller, null, new { Class = ViewBag.CurrentTab == @m.Name ? "tab_current" : "" })
        }
    }
</div>